#include <stdio.h>

void replaceColumnsWithRows(int n, int A[][n], int B[][n]) {
    int i, j, k;
    
    // Виведення матриці А
    printf("Матриця А:\n");
    for (i = 0; i < n; i++) {
        for (j = 0; j < n; j++) {
            printf("%d ", A[i][j]);
        }
        printf("\n");
    }
    
    printf("\n");
    
    // Заміна парних стовпців на непарні рядки з матриці В
    for (i = 0; i < n; i++) {
        if (i % 2 == 0) {
            k = i / 2;
            for (j = 0; j < n; j++) {
                A[j][i] = B[k][j];
            }
        }
    }
    
    // Виведення отриманої матриці А
    printf("Матриця А замінена:\n");
    for (i = 0; i < n; i++) {
        for (j = 0; j < n; j++) {
            printf("%d ", A[i][j]);
        }
        printf("\n");
    }
}

int main() {
    int n = 4;
    int A[4][4] = {
        {4, 6, 4, 3},
        {2, 33, 4, 1},
        {3, 8, 0, 22},
        {3, 1, 5, 12}
    };
    int B[4][4] = {
        {13, 14, 15, 16},
        {9, 10, 11, 12},
        {5, 6, 7, 8},
        {1, 2, 3, 4}
    };
    
    replaceColumnsWithRows(n, A, B);
    
    return 0;
}
